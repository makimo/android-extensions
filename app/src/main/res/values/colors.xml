<?xml version="1.0" encoding="utf-8"?>
<resources>

    <color name="color1">#00DDDC</color>
    <color name="color2">#00C0BF</color>
    <color name="color3">#43A1D5</color>
    <color name="color4">#FFFF</color>

    <!-- Color for the app bar and other primary UI elements. -->
    <color name="colorPrimary">@color/color1</color>

    <!-- A darker variant of the primary color, used for
         the status bar and contextual app bars. -->
    <color name="colorPrimaryDark">@color/color2</color>

    <!-- A secondary color for controls like checkboxes and text fields. -->
    <color name="colorAccent">@color/color3</color>

</resources>
